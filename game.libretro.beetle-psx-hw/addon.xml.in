<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.beetle-psx-hw"
		name="Sony - PlayStation (Beetle PSX HW)"
		version="0.9.44.10"
		provider-name="Mednafen Team">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.ps.gamepad" version="1.0.0"/>
		<import addon="game.controller.ps.dualshock" version="1.0.0"/>
